#!/usr/bin/make -f
# See debhelper(7) (uncomment to enable)
# output every command that modifies files on the build system.
export DH_VERBOSE = 1

%:
	dh $@

override_dh_auto_build:
	dh_auto_build -- insteadman insteadman-gtk

# Eliminate test target because of dependency trouble
override_dh_auto_test:
	echo 'Skipping tests'
